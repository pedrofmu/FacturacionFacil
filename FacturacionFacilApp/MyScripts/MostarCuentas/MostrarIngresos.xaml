<Window x:Class="FacturacionFacilApp.MyScripts.MostarCuentas.MostrarIngresos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FacturacionFacilApp.MyScripts.MostarCuentas"
        mc:Ignorable="d"
        Title="MostrarIngresos" Height="450" Width="800">
    <Grid>
        <DataGrid ItemsSource="{Binding YourDataCollection}" Margin="74,68,226,148">
            <!-- Columnas -->
            <DataGrid.Columns>
                <DataGridTextColumn Header="Nombre" Binding="{Binding Name}" />
                <DataGridTextColumn Header="Edad" Binding="{Binding Age}" />
                <!-- Agrega más columnas según tus necesidades -->
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
